{
	"name": "vinkla/translator",
	"description": "Laravel translator for multilingual Eloquent objects.",
	"homepage": "https://github.com/vinkla/translator",
	"keywords": ["translations", "translator", "laravel", "language", "multilingual", "translation"],
	"license": "MIT",
	"authors": [
		{
			"name": "Vincent Klaiber",
			"homepage": "http://vinkla.com"
		}
	],
	"require": {
		"php": ">=5.4.7",
		"illuminate/support": "~5.0"
	},
	"require-dev": {
		"graham-campbell/testbench": "~2.0"
	},
	"autoload": {
		"psr-0": {
			"Vinkla\\Translator": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Vinkla\\Tests\\Translator\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
